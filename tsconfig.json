{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*",
        "./*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@state/*": [
        "state/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "src/types/env.d.ts",
    ".expo/**/*.ts",
    ".expo/**/*.tsx",
    ".expo/**/*.d.ts"
  ]
}